{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "wordpress",
    "eslint:recommended",
    "plugin:jquery/slim",
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier/react"
  ],
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "wp": true,
    "wpApiSettings": true,
    "window": true,
    "document": true,
    "hljs": true,
    "selectedFonts": true,
    "WebFont": true,
    "$": true,
    "location": true,
    "alert": true,
    "gsap": true,
    "Power2": true,
    "ScrollMagic": true
  },
  "plugins": ["jquery", "react", "jsx-a11y"],
  "settings": {
    "react": {
      "pragma": "wp",
      "version": "detect"
    }
  },
  "rules": {
    "no-console": 0,
    "jquery/no-hide": 0,
    "jquery/no-show": 0,
    "react/prop-types": 0,
    "react/jsx-key": 0,
    "react/display-name": 0
  }
}